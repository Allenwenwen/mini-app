/* components/tab-control/tab-control.wxss */
.tab-container {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  background: orange;
}
.tab-item {
  flex: 1;
  text-align: center;
  color: #fff;
}
.active{
  color: red;
}
.active text {
  padding: 20rpx 30rpx;
  border-bottom: 6rpx solid red;
}











